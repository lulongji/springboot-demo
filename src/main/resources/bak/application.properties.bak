#默认使用配置
spring.profiles.active=dev
#公共配置与profiles选择无关
mybatis.typeAliasesPackage=com.example.demo.domain
mybatis.mapperLocations=classpath*=com/example/demo/dao/**/*Mapper.xml
#项目通用应用
test=Hello world!
#日志
logback.log-dir=/Users/lu/Desktop
logback.log-file=demo
#开发配置
server.port=8080
server.session=
#上传路径
web.upload-path=/Users/lu/Desktop
#
spring.profiles=dev
spring.datasource.url=jdbc=oracle=thin=@192.168.6.205=1521=orcl
spring.datasource.username=test
spring.datasource.password=test
spring.datasource.driver-class-name=oracle.jdbc.driver.OracleDriver
spring.datasource.druid.initialSize=5
spring.datasource.druid.maxActive=100
spring.datasource.druid.minIdle=10
spring.datasource.druid.maxWait=60000
#使用druid数据源
spring.datasource.type=com.alibaba.druid.pool.DruidDataSource




#redis
redis.database=0
redis.ip=192.168.6.205
redis.pwd=
redis.port=6379
# 连接超时时间 单位 ms（毫秒）
redis.timeout=20000
redis.pool.maxIdle=150
#连接池中的最小空闲连接，默认值也是0。
redis.pool.minIdle=100
# 如果赋值为-1，则表示不限制；如果pool已经分配了maxActive个jedis实例，则此时pool的状态为exhausted(耗尽)。
redis.pool.maxActive=1024
# 等待可用连接的最大时间，单位毫秒，默认值为-1，表示永不超时。如果超过等待时间，则直接抛出JedisConnectionException
redis.pool.max-wait=1000
#多长时间检查一次连接池中空闲的连接
redis.pool.maxTotal=150
#当池内没有返回对象时，最大等待时间
redis.pool.maxWait=7000
#当调用borrowObject方法时，是否进行有效性检查
redis.pool.testOnBorrow=true
#重新连接重试次数
redis.pool.reconnectRetryCount=50
#重连等待时间
redis.pool.reconnectRetryWaittime=7000



pagehelper.helperDialect=oracle
pagehelper.reasonable=true
pagehelper.supportMethodsArguments=true
pagehelper.params=count=countSql
pagehelper.offset-as-page-num=true
pagehelper.row-bounds-with-count=true

